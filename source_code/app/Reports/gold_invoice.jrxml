		<query language="SQL"><![CDATA[SELECT 
	SOP.id as pay_id,
    CASE WHEN SOP.pay_method = 1 THEN 'Thảo' 
			   WHEN SOP.pay_method = 2 THEN 'Dẻ' 
               WHEN SOP.pay_method = 3 THEN 'Tiền mặt' 
               WHEN SOP.pay_method = 4 THEN 'Đúc' 
               WHEN SOP.pay_method = 5 THEN 'Bộng' 
               ELSE '' 
    END as pay_method,
	SOP.pay_method as pay_method_id,
	description,
    qty,
    total_weight,
    gem_weight,
    gold_weight,
    money,
    gold_age,
    converted_price,
    exchange_g10
FROM gold_sale_order_pays as SOP
WHERE SOP.order_id =  $P{order_id} 
ORDER BY SOP.id]]></query>
