	<parameter name="id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT 
	SO.id as order_id,
    SO.order_no,
    SO.order_date,
    SO.saler_id,
    U.employee_code as saler_code,
    U.name as saler_name,
    SO.customer_id,
    CASE WHEN C.code is null THEN C.tmp_code ELSE C.code END as customer_code,
    C.name as customer_name,
    (SELECT count(*) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) <> 'V_' AND upper(substring(P.product_code,1,2)) <> 'B_'  AND SOD.order_id = SO.id ) as d1_qty,
    IFNULL((SELECT sum(P.total_weight) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) <> 'V_' AND upper(substring(P.product_code,1,2)) <> 'B_'  AND SOD.order_id = SO.id ),0) as d1_total_weight,
    IFNULL((SELECT sum(P.gem_weight) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) <> 'V_' AND upper(substring(P.product_code,1,2)) <> 'B_'  AND SOD.order_id = SO.id ),0) as d1_total_gem_weight,
    IFNULL((SELECT sum(P.gold_weight) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) <> 'V_' AND upper(substring(P.product_code,1,2)) <> 'B_'  AND SOD.order_id = SO.id ),0) as d1_total_gold_weight,
    IFNULL((SELECT sum(P.retail_machining_fee) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) <> 'V_' AND upper(substring(P.product_code,1,2)) <> 'B_'  AND SOD.order_id = SO.id ),0) as d1_total_wage,
    SO.gold_age_1,
    (SELECT count(*) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) = 'B_' AND SOD.order_id = SO.id ) as d2_qty,
    IFNULL((SELECT sum(P.total_weight) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) = 'B_' AND SOD.order_id = SO.id ),0) as d2_total_weight,
    IFNULL((SELECT sum(P.gem_weight) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) = 'B_' AND SOD.order_id = SO.id ),0) as d2_total_gem_weight,
    IFNULL((SELECT sum(P.gold_weight) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) = 'B_' AND SOD.order_id = SO.id ),0) as d2_total_gold_weight,
    IFNULL((SELECT sum(P.retail_machining_fee) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) = 'B_' AND SOD.order_id = SO.id ),0) as d2_total_wage,
    SO.gold_age_2,
    (SELECT count(*) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) = 'V_' AND SOD.order_id = SO.id ) as d3_qty,
    IFNULL((SELECT sum(P.total_weight) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) = 'V_' AND SOD.order_id = SO.id ),0) as d3_total_weight,
    IFNULL((SELECT sum(P.gem_weight) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) = 'V_' AND SOD.order_id = SO.id ),0) as d3_total_gem_weight,
    IFNULL((SELECT sum(P.gold_weight) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) = 'V_' AND SOD.order_id = SO.id ),0) as d3_total_gold_weight,
    IFNULL((SELECT sum(P.retail_machining_fee) FROM gold_sale_order_details as SOD LEFT JOIN gold_products as P ON SOD.product_id = P.id WHERE upper(substring(P.product_code,1,2)) = 'V_' AND SOD.order_id = SO.id ),0) as d3_total_wage,
    SO.gold_age_3,
    SO.sampling_discount,
    SO.reduce,
    SO.actual_weight,
    DATE_FORMAT(SO.debt_date, '%d/%m/%Y') as debt_date,
    SO.days_diff,
	SO.exchange_g10,
    SO.wage,
    IFNULL((SELECT SOR.qty FROM gold_sale_order_returns as SOR WHERE SOR.type = 1 AND SOR.order_id = SO.id limit 1),0) as r1_qty,
    IFNULL((SELECT SOR.total_weight FROM gold_sale_order_returns as SOR WHERE SOR.type = 1 AND SOR.order_id = SO.id  limit 1),0) as r1_total_weight,
    IFNULL((SELECT SOR.gem_weight FROM gold_sale_order_returns as SOR WHERE SOR.type = 1 AND SOR.order_id = SO.id  limit 1),0) as r1_total_gem_weight,
    IFNULL((SELECT SOR.gold_weight FROM gold_sale_order_returns as SOR WHERE SOR.type = 1 AND SOR.order_id = SO.id  limit 1),0) as r1_total_gold_weight,
    IFNULL((SELECT SOR.gold_age FROM gold_sale_order_returns as SOR WHERE SOR.type = 1 AND SOR.order_id = SO.id  limit 1),0) as r1_total_gold_age,
    IFNULL((SELECT SOR.exchange_g10 FROM gold_sale_order_returns as SOR WHERE SOR.type = 1 AND SOR.order_id = SO.id  limit 1),0) as r1_total_exchange_g10,
    IFNULL((SELECT SOR.wage FROM gold_sale_order_returns as SOR WHERE SOR.type = 1 AND SOR.order_id = SO.id  limit 1),0) as r1_total_wage,
    IFNULL((SELECT SOR.qty FROM gold_sale_order_returns as SOR WHERE SOR.type = 2 AND SOR.order_id = SO.id limit 1),0) as r2_qty,
    IFNULL((SELECT SOR.total_weight FROM gold_sale_order_returns as SOR WHERE SOR.type = 2 AND SOR.order_id = SO.id  limit 1),0) as r2_total_weight,
    IFNULL((SELECT SOR.gem_weight FROM gold_sale_order_returns as SOR WHERE SOR.type = 2 AND SOR.order_id = SO.id  limit 1),0) as r2_total_gem_weight,
    IFNULL((SELECT SOR.gold_weight FROM gold_sale_order_returns as SOR WHERE SOR.type = 2 AND SOR.order_id = SO.id  limit 1),0) as r2_total_gold_weight,
    IFNULL((SELECT SOR.gold_age FROM gold_sale_order_returns as SOR WHERE SOR.type = 2 AND SOR.order_id = SO.id  limit 1),0) as r2_total_gold_age,
    IFNULL((SELECT SOR.exchange_g10 FROM gold_sale_order_returns as SOR WHERE SOR.type = 2 AND SOR.order_id = SO.id  limit 1),0) as r2_total_exchange_g10,
    IFNULL((SELECT SOR.wage FROM gold_sale_order_returns as SOR WHERE SOR.type = 2 AND SOR.order_id = SO.id  limit 1),0) as r2_total_wage,
    IFNULL((SELECT SOR.qty FROM gold_sale_order_returns as SOR WHERE SOR.type = 3 AND SOR.order_id = SO.id limit 1),0) as r3_qty,
    IFNULL((SELECT SOR.total_weight FROM gold_sale_order_returns as SOR WHERE SOR.type = 3 AND SOR.order_id = SO.id  limit 1),0) as r3_total_weight,
    IFNULL((SELECT SOR.gem_weight FROM gold_sale_order_returns as SOR WHERE SOR.type = 3 AND SOR.order_id = SO.id  limit 1),0) as r3_total_gem_weight,
    IFNULL((SELECT SOR.gold_weight FROM gold_sale_order_returns as SOR WHERE SOR.type = 3 AND SOR.order_id = SO.id  limit 1),0) as r3_total_gold_weight,
    IFNULL((SELECT SOR.gold_age FROM gold_sale_order_returns as SOR WHERE SOR.type = 3 AND SOR.order_id = SO.id  limit 1),0) as r3_total_gold_age,
    IFNULL((SELECT SOR.exchange_g10 FROM gold_sale_order_returns as SOR WHERE SOR.type = 3 AND SOR.order_id = SO.id  limit 1),0) as r3_total_exchange_g10,
    IFNULL((SELECT SOR.wage FROM gold_sale_order_returns as SOR WHERE SOR.type = 3 AND SOR.order_id = SO.id  limit 1),0) as r3_total_wage,
    DATE_FORMAT(SO.pay_date, '%d/%m/%Y') as pay_date,
    SO.pay_total_wage,
    IFNULL(SO.pay_total_wage,0) + IFNULL((SELECT SUM(IFNULL(SOP.money,0)) FROM gold_sale_order_pays as SOP WHERE SOP.pay_method = 3 AND  SOP.order_id = SO.id), 0) as pay_total_money_and_wage,
    IFNULL((SELECT SUM(SOP.exchange_g10) FROM gold_sale_order_pays as SOP WHERE SOP.order_id = SO.id),0) as pay_total_exchange_g10,
    SO.total_exchange_g10,
    SO.total_wage,
    SO.reason_pay_not_enough,
    SO.other_orders
FROM gold_sale_orders as SO
LEFT JOIN cms_users as U ON SO.saler_id = U.id
LEFT JOIN gold_customers as C ON SO.customer_id = C.id
WHERE SO.id = $P{id}]]>
	</queryString>
